@model List<TodoConsumeAPI.Models.TodoItem>
@{
    ViewData["Title"] = "API Data";
}

@if (Model.Count() > 0)
{
    <h3>@ViewData["Title"]</h3>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Is complete</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.IsComplete</td>
            </tr>
        }
    </table>
}
else
{
    <p>No data :-)</p>
}